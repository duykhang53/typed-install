{
  "name": "typed-install",
  "version": "1.0.5",
  "description": "Install packages and types in one fell swoop.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "xavdid/typed-install",
  "bin": {
    "typedi": "lib/cli.js"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "commander": "2.12.2",
    "got": "8.0.0",
    "lodash": "4.17.4",
    "mz": "2.7.0",
    "ora": "2.1.0",
    "pkg-dir": "2.0.0",
    "shelljs": "0.7.8",
    "update-notifier": "3.0.0"
  },
  "devDependencies": {
    "@types/got": "^7.1.5",
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.117",
    "@types/mock-fs": "^3.6.30",
    "@types/mz": "0.0.32",
    "@types/ora": "1.3.4",
    "@types/pkg-dir": "^2.0.0",
    "@types/shelljs": "0.7.6",
    "@types/update-notifier": "2.5.0",
    "jest": "^22.4.3",
    "mock-fs": "^4.4.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "2.7.2"
  },
  "scripts": {
    "test": "jest",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "posttest": "npm run lint",
    "predeploy": "rm -rf lib && npm rum build",
    "deploy": "npx np",
    "build": "tsc",
    "release": "npx np"
  },
  "keywords": [
    "typescript",
    "npm",
    "yarn",
    "installing"
  ],
  "author": "David Brownman <beamneocube@gmail.com> (https://davidbrownman.com)",
  "license": "ISC",
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "files": [
    "lib/*.js",
    "lib/index.d.ts"
  ],
  "jest": {
    "silent": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
